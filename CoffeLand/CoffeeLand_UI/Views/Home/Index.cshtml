            <div class="inner-sidebar">
                <div class="menu-top">
                    <a class="close-sidebar" href=""></a>
                    <a href="@Url.Action("Index","Home")" class="menu-logo" style="font-family: 'Dancing Script', cursive;position:relative;font-size:1.5em;top:100px;">
                        <div style="font-size:40pt;">
                            CoffeeLand
                        </div>
                    </a>
                </div>
                <ul>
                    <li>
                        @Html.ActionLink("KAHVELERİMİZ", "Coffees", "Coffee")
                    </li>
                    <li>
                        @Html.ActionLink("HAKKIMIZDA", "About", "Home")
