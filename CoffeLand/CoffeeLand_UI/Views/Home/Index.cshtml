            username = (Session["OnlineKullanici"] as CoffeeLand_DATA.Classes.Customer).UserName;
        }
    }

    <div id="container" class="without-padding">
        <header class="clearfix" id="main-header">
            <div class="navbar navbar-default">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                            <span class="sr-only"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand " href="@Url.Action("Index","Home")" style="float:left;position:relative;">
                            <div class="coffee-cup">
                                <div class="handle">
                                </div>
                                <div class="yazi" style="font-size:30pt;">
                                    CoffeeLand
                                </div>
                            </div>
                        </a>
                      
                    </div>
                    <div id="navbar" class="navbar-collapse collapse">
                        <a href="#" class="open-menu">
                            <span class="opened"></span>
                            <span class="opened"></span>
                            <span class="opened"></span>
                        </a>
                        <ul class="nav navbar-nav"></ul>
                        <ul class="nav navbar-nav navbar-right" id="menuu">
                            <li class="list-unstyled">
                                @Html.ActionLink("KAHVELER", "Coffees", "Coffee")
                            </li>
                            <li class="list-unstyled">
                                @Html.ActionLink("HAKKIMIZDA", "About", "Home")
                            </li>
                            <li class="list-unstyled">
                                @Html.ActionLink("İLETİŞİM", "Contact", "Home")
                            </li>
                            <li class="list-unstyled">
                                @{
                                    if (Session["OnlineKullanici"] == null)
                                    {
                                        <a href="@Url.Action("Login","Login")">GİRİŞ YAP</a>
                                    }
                                    else
                                    {
                                        <a href="@Url.Action("Account","Account", new { username = username})">PROFİL SAYFASI</a>
                                    }
                                }
                            </li>
                        </ul>
                    </div>






